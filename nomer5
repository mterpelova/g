#include <iostream>

using namespace std;

int main(){
    int a,b;
    b=1;
    while(1){
        cin>>a;
        if(a%2==0){
            if(a>b){
                b=a;
            }
        }
        if(a%10==0){
            break;
        }
    }
    if(b==1){
        cout<<"No";
    }else{
        cout<<b;
    }
}
